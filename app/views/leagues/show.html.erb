
<div class="container">
  <div class="form-login" id="form-size">
    <div class="form-inputs form_info">

      <div class="form-actions">
      <h1><%= @league.name %></h1>
      <h3><%= @league.sport %></h3>

      <p><%= invitation_url %></p>

      <p><%= link_to "See matches", league_matches_path(@league) %></p>

      <% @league.users.each do |user| %>
      <p><%= user.email %></p>

      <% end %>

    </div>
  </div>
 </div>
</div>
